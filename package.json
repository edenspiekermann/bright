{
  "name": "videoplayer",
  "version": "0.0.1",
  "description": "easy videoplayer abstraction",
  "main": "src/videoplayer.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "webpack src/videoplayer.js dist/videoplayer.standalone.js --output-library videoplayer --output-library-target umd --watch --progress --colors",
    "build": "npm run build-common && npm run build-common-min && npm run build-standalone && npm run build-standalone-min",
    "build-common": "webpack src/videoplayer.js dist/videoplayer.js",
    "build-common-min": "webpack src/videoplayer.js dist/videoplayer.min.js --optimize-minimize",
    "build-standalone": "webpack src/videoplayer.js dist/videoplayer.standalone.js --output-library videoplayer --output-library-target umd",
    "build-standalone-min": "webpack src/videoplayer.js dist/videoplayer.standalone.min.js --output-library videoplayer --output-library-target umd --optimize-minimize",
    "test": "open 'http://localhost:8000/tests/index.html' && python -m SimpleHTTPServer"
  },
  "author": "edenspiekermann",
  "license": "MIT",
  "devDependencies": {
    "webpack": "^1.3.1-beta1"
  },
  "dependencies": {
    "component-emitter": "^1.1.2",
    "lodash-node": "^2.4.1"
  }
}
